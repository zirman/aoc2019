{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "array-type": [true, "generic"],
        "no-console": [false],
        "quotemark": [true, "single"],
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "strict-boolean-expressions": true,
        "interface-name": false,
        "no-bitwise": false,
        "no-empty": false
    },
    "rulesDirectory": []
}
